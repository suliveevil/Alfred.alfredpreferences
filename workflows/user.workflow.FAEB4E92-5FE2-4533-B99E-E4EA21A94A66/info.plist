				<key>script</key>
				<string>set workflowFolder to do shell script "pwd"
set wlib to load script POSIX file (workflowFolder &amp; "/q_workflow.scpt")
set wf to wlib's new_workflow_with_bundle("com.milkticc.ToggleInputMethod")tell application "System Events" to tell process "SystemUIServer"		set menuA to (1st menu bar item of menu bar 1 whose description is "text input")	set iML to name of menu items of menu 1 of menuA	set mPos to 0	set sIM to ""	repeat with a from 1 to count of iML		if item a of iML is missing value then			set mPos to a			exit repeat		end if	end repeat			set iML1 to items 1 thru (mPos - 1) of iML			repeat with mItem in menu items of menu 1 of menuA		if value of attribute "AXMenuItemMarkChar" of mItem as string = "âœ“" then			set sIM to name of mItem			exit repeat		end if	end repeat	end tellrepeat with i from 1 to the count of iML1
set n to item i of iML1
if item i of iML1 = sIM then
	add_result of wf with isValid given theUid:"", theArg:i, theTitle:n, theAutocomplete:"", theSubtitle:n &amp; "is Currently Selected", theIcon:"icon.png", theType:""
else
	add_result of wf with isValid given theUid:"", theArg:i, theTitle:n, theAutocomplete:"", theSubtitle:"Select "&amp;n, theIcon:"icon.png", theType:""
end if
end repeat
wf's to_xml("")</string>
